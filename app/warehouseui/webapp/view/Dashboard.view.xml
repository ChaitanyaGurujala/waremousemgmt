<mvc:View
    controllerName="warehouseui.controller.Dashboard"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:f="sap.f"
    xmlns:card="sap.f.cards"
    xmlns:core="sap.ui.core">
    
    <Page title="Warehouse Management Dashboard" class="sapUiResponsiveMargin">
        <content>
            <GenericTile
                class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                header="Product Performance"
                subheader="View critical inventory metrics"
                press=".onNavToProductPerformance">
                <TileContent>
                    <ImageContent src="sap-icon://product" />
                </TileContent>
            </GenericTile>

            <GenericTile
                class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                header="Inventory Lookup"
                subheader="Check product quantities"
                press=".onNavToInventoryLookup">
                <TileContent>
                    <ImageContent src="sap-icon://search" />
                </TileContent>
            </GenericTile>

            <GenericTile
                class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                header="Sales Orders"
                subheader="Manage customer orders"
                press=".onNavToSalesOrders">
                <TileContent>
                    <ImageContent src="sap-icon://sales-order" />
                </TileContent>
            </GenericTile>

            <!-- Add this tile to your existing Dashboard.view.xml -->
<GenericTile
    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
    header="Inventory by Location"
    subheader="View stock levels across locations"
    press=".onNavToInventoryByLocation">
    <TileContent>
        <ImageContent src="sap-icon://map" />
    </TileContent>
</GenericTile>

            <GenericTile
                class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                header="Purchase Orders"
                subheader="Manage vendor orders"
                press=".onNavToPurchaseOrders">
                <TileContent>
                    <ImageContent src="sap-icon://supplier" />
                </TileContent>
            </GenericTile>
        </content>
    </Page>
</mvc:View>